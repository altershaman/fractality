{
  "name": "fractality",
  "displayName": "fractality",
  "description": "fractality language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "fractality",
      "aliases": ["fractality", "fractality"],
      "extensions": ["fractal"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/language_16x_icon_black.svg",
        "dark": "./icons/language_16x_icon_white.svg"
      }
    }],
    "grammars": [{
      "language": "fractality",
      "scopeName": "source.fractality",
      "path": "./syntaxes/fractality.tmLanguage.json"
    }]
  }
}
